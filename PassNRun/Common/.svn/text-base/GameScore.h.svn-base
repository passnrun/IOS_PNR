#import <Foundation/Foundation.h>

@interface GameScore : NSObject
{
    NSString * homeTeam;
    NSString * awayTeam;
    int homeScore;
    int awayScore;
    NSArray * gameScoreItems;
}
@property (nonatomic, strong) NSString * homeTeam;
@property (nonatomic, strong) NSString * awayTeam;
@property (nonatomic, assign) int homeScore;
@property (nonatomic, assign) int awayScore;
@property (nonatomic, strong) NSArray * gameScoreItems;


@end
